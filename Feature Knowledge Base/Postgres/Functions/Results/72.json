    "Title": [
        "concat"
    ],
    "Feature": [
        "concat ( val1 \"any\" [, val2 \"any\" [, ...] ] ) → text"
    ],
    "Description": [
        "Concatenates the text representations of all the arguments. NULL arguments are ignored."
    ],
    "Examples": [
        "concat('abcde', 2, NULL, 22) → abcde222"
    ],
    "Category": [
        "String Functions and Operators"
    ]
}