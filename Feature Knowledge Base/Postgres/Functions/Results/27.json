    "Title": [
        "trim_scale"
    ],
    "Feature": [
        "trim_scale ( numeric ) → numeric"
    ],
    "Description": [
        "Reduces the value's scale (number of fractional decimal digits) by removing trailing zeroes"
    ],
    "Examples": [
        "trim_scale(8.4100) → 8.41"
    ],
    "Category": [
        "Mathematical Functions and Operators"
    ]
