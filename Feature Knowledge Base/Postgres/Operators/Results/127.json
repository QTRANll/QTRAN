    "Title": [
        "jsonb - text"
    ],
    "Feature": [
        "jsonb - text → jsonb"
    ],
    "Description": [
        "Deletes a key (and its value) from a JSON object, or matching string value(s) from a JSON array."
    ],
    "Examples": [
        "'{\"a\": \"b\", \"c\": \"d\"}'::jsonb - 'a' → {\"c\": \"d\"}",
        "'[\"a\", \"b\", \"c\", \"b\"]'::jsonb - 'b' → [\"a\", \"c\"]"
    ],
    "Category": [
        "JSON Functions and Operators"
    ]
}