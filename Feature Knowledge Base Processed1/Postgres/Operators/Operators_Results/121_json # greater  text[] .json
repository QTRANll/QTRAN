    "Title": [
        "json #> text[]"
    ],
    "Feature": [
        "json #> text[] → json",
        "jsonb #> text[] → jsonb"
    ],
    "Description": [
        "Extracts JSON sub-object at the specified path, where path elements can be either field keys or array indexes."
    ],
    "Examples": [
        "'{\"a\": {\"b\": [\"foo\",\"bar\"]}}'::json #> '{a,b,1}' → \"bar\""
    ],
    "Category": [
        "JSON Functions and Operators"
    ]
