    "Feature": [
        "SQRT(X)\n"
    ],
    "Description": [
        "Returns the square root of X. If X is negative, NULL is returned."
    ],
    "Examples": [
        "SELECT SQRT(4);",
        "SELECT SQRT(20);",
        "SELECT SQRT(-16);",
        "SELECT SQRT(1764);"
    ],
