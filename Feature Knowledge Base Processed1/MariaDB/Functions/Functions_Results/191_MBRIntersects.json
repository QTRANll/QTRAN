    "Title": [
        "MBRIntersects"
    ],
    "Feature": [
        "MBRIntersects(g1,g2)\n"
    ],
    "Description": [
        "Returns 1 or 0 to indicate whether the Minimum Bounding Rectangles of the two geometries g1 and g2 intersect."
    ],
    "Examples": [
        "SELECT mbrintersects(@g1,@g2);",
        "SELECT mbrintersects(@g1,@g2);"
    ],
    "Category": [
        "Geographic Functions"
    ]
}