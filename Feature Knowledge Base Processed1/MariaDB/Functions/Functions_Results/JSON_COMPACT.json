    "Feature": [
        "JSON_COMPACT(json_doc)\n"
    ],
    "Description": [
        "Removes all unnecessary spaces so the json document is as short as possible."
    ],
    "Examples": [
        "SET @j = '{ \"A\": 1, \"B\": [2, 3]}';\n\nSELECT JSON_COMPACT(@j), @j;\n+-------------------+------------------------+\n| JSON_COMPACT(@j)  | @j                     |\n+-------------------+------------------------+\n| {\"A\":1,\"B\":[2,3]} | { \"A\": 1, \"B\": [2, 3]} |\n+-------------------+------------------------+\n"
