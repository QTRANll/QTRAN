    "Title": [
        "CURDATE"
    ],
    "Feature": [
        "CURDATE()\nCURRENT_DATE\nCURRENT_DATE()\n"
    ],
    "Description": [
        "CURDATE returns the current date as a value in 'YYYY-MM-DD' or YYYYMMDD\nformat, depending on whether the function is used in a string or\nnumeric context.",
        "CURRENT_DATE and CURRENT_DATE() are synonyms."
    ],
    "Examples": [
        "SELECT CURDATE();",
        "SELECT CURDATE() +0;",
        "SELECT CURDATE() - INTERVAL 5 DAY;"
    ],
    "Category": [
        "Date & Time Functions"
    ]
}