{
    "HTML": [
        "https://mariadb.com//kb/en/last_day/"
    ],
    "Title": [
        "LAST_DAY"
    ],
    "Feature": [
        "LAST_DAY(date)\n"
    ],
    "Description": [
        "Takes a date or datetime value and returns the corresponding value for\nthe last day of the month. Returns NULL if the argument is invalid."
    ],
    "Examples": [
        "SELECT LAST_DAY('2003-02-05');",
        "SELECT LAST_DAY('2004-02-05');",
        "SELECT LAST_DAY('2004-01-01 01:01:01');",
        "SELECT LAST_DAY('2003-03-32');"
    ],
    "Category": [
        "Date & Time Functions"
    ]
}