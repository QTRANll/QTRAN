    "Title": [
        "SIGN"
    ],
    "Feature": [
        "SIGN(X)\n"
    ],
    "Description": [
        "Returns the sign of the argument as -1, 0, or 1, depending on whether\nX is negative, zero, or positive."
    ],
    "Examples": [
        "SELECT SIGN(-32);",
        "SELECT SIGN(0);",
        "SELECT SIGN(234);"
    ],
    "Category": [
        "Numeric Functions"
    ]
}