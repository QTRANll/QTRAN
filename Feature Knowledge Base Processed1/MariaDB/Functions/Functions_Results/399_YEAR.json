    "Title": [
        "YEAR"
    ],
    "Feature": [
        "YEAR(date)\n"
    ],
    "Description": [
        "Returns the year for the given date, in the range 1000 to 9999, or 0 for the\n\"zero\" date."
    ],
    "Examples": [
        "SELECT YEAR('1987-01-01');"
    ],
    "Category": [
        "Date & Time Functions"
    ]
}