    "Feature": [
        "tsquery && tsquery → tsquery"
    ],
    "Description": [
        "ANDs two tsquerys together, producing a query that matches documents that match both input queries."
    ],
    "Examples": [
        "'fat | rat'::tsquery && 'cat'::tsquery → ( 'fat' | 'rat' ) & 'cat'"
    ],
